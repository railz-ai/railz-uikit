{
  "name": "@railzai/railz-icons",
  "version": "0.0.1-alpha.41",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/railz-icons/railz-icons.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test:stencil": "stencil test --spec",
    "test:all": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "eslint src/**/*{.ts,.tsx} --fix",
    "stylelint": "stylelint 'src/**/*.scss' --fix",
    "test:lint:ts": "eslint --ext .ts,.tsx --ignore-path ../../../.gitignore",
    "test:lint:scss": "stylelint 'src/**/*.scss'",
    "test": "yarn run test:lint:ts && yarn run test:lint:scss && yarn run test:stencil",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "0.4.0",
    "@types/jest": "~27.4.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "2.5.1",
    "puppeteer": "^10.0.0",
    "stylelint": "14.5.3",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.1.0",
    "typescript": "4.4.4"
  },
  "license": "MIT",
  "gitHead": "a10e4f996b6135505df71725f3d2a65e274a6359"
}
