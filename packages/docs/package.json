{
  "name": "railz-uikit-docs",
  "version": "0.0.1-alpha.1",
  "description": "Documentation for Railz UI Kit",
  "private": true,
  "author": "railz.ai",
  "scripts": {
    "build": "stencil build",
    "dev": "LOCAL_DEV=true stencil build --dev --watch --serve",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate:icons": "node SVGtoJson_glob.js",
    "cp:version": "rsync -av ./www/railz-uikit ./www/railz-uikit/$VERSION/",
    "clean": "rm -rf www/",
    "prestart": "yarn clean",
    "deploy": "gh-pages -t -d www/railz-uikit -b gh-pages"
  },
  "lint-staged": {
    "*.scss": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@stencil-community/router": "1.0.2",
    "@stencil/core": "4.5.0",
    "@stencil/helmet": "^0.3.3",
    "@stencil/sass": "^3.0.6",
    "@types/marked": "^4.0.3",
    "del-cli": "~4.0.1",
    "doctrine": "^3.0.0",
    "gh-pages": "^4.0.0",
    "marked": "^4.0.18",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stencil-inline-svg": "^1.1.0",
    "svgson": "^5.2.1",
    "uuid": "^8.3.2",
    "workbox-build": "^6.5.3"
  },
  "license": "MIT",
  "dependencies": {
    "@railzai/railz-uikit": "^0.0.1-alpha.54",
    "@rollup/plugin-alias": "^5.0.1",
    "fs-extra": "^10.0.1",
    "glob-promise": "^4.2.2",
    "path": "^0.12.7",
    "pretty": "^2.0.0",
    "util": "^0.12.5"
  }
}
